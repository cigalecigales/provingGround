num1 = 1234567890123456789012345678901
puts num1.class

num2 = 123456789012345678901234567890
puts num2.class

num3 = 1234567890123456789
puts num3.class

num4 = 9999999999999999999
puts num4.class

puts num4.class == Bignum

sum = 4611005000000000000
while sum.class == Fixnum do
	sum += 1
	puts sum
end

