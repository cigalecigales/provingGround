<p id="notice"><%= notice %></p>

<h1>Listing Cicadas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cicadas.each do |cicada| %>
      <tr>
        <td><%= cicada.title %></td>
        <td><%= cicada.description %></td>
        <td><%= cicada.address %></td>
        <td><%= cicada.latitude %></td>
        <td><%= cicada.longitude %></td>
        <td><%= link_to 'Show', cicada %></td>
        <td><%= link_to 'Edit', edit_cicada_path(cicada) %></td>
        <td><%= link_to 'Destroy', cicada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cicada', new_cicada_path %>
